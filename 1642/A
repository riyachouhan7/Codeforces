#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define endl '\n'
int main()
{
    ll t;cin>>t;
    while(t--){
        ll x1,y1;cin>> x1>>y1;
        ll x2,y2;cin>> x2>>y2;
        ll x3,y3;cin>> x3>>y3;
        if (y1==y2 && y3 < y2){
            cout << abs(x1-x2);
        }
        else if (y3==y2 && y1 < y2){
            cout << abs(x3-x2);
        }
        else if (y1==y3 && y2 < y3){
            cout << abs(x1-x3);
        }
        else
            cout << 0;
        cout<< endl;
        }
    return 0;
}
